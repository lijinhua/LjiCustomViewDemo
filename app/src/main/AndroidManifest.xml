<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lijinhua.ljicustomviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.FaceBookCustomViewGroupActivity" />
        <activity android:name=".ui.mediumnet.CustomViewGroupActivity" />
        <activity android:name=".ui.mediumnet.CustomViewActivity" />
        <activity android:name=".ui.google.GoogleCustomViewActivity" />
        <activity android:name=".ui.google.GoogleCustomGroupActivity" />
        <activity android:name=".ui.other.TagLayoutActivity"></activity>
    </application>

</manifest>